{
	"$schema": "https://opencode.ai/config.json",
	//"model": "chutes/zai-org/GLM-5-TEE",
	"theme": "system",
	"compaction": {
		"auto": true,
		"prune": true,
		"threshold": 0.5,
		//"preserveLastN": 5
	},
	// add instructions, perhaps some general or project specific on style and what not
	// create instructions for a project.md file per project which the llm can write to
	"provider": {
		"openrouter": {
			"models": { 
				"qwen/qwen3-coder-480b-a35b:floor": {},
				"minimax/minimax-m2.1:floor": {},
				"z-ai/glm-4.7:floor": {},
				"z-ai/glm-4.6:floor": {},
				"openai/gpt-oss-120b:floor": {},
				"xiaomi/mimo-v2-flash:floor": {},
				"moonshotai/kimi-k2-thinking:floor": {},
				"Venice-GLM-4.7": {
					"id": "z-ai/glm-4.7",
					"options": {
						"provider": {
							"order": ["Venice"],
							"allow_fallbacks": false,
							"data_collection": "deny"
						}
					}
				}
			}
		},
		"zai-coding-plan": {
		},
		"ollama": {
			"npm": "@ai-sdk/openai-compatible",
			"options": {
				"baseURL": "http://strix.netbird.cloud:21339/v1"
	 		},
			"models": {
				"glm-4.7-flash": {
					"name": "glm-4.7-flash"
				}
			}
		},
		"chutes": {
			"npm": "@ai-sdk/openai-compatible",
			"options": {
				"baseURL": "https://llm.chutes.ai/v1",
				"apiKey": "{env:CHUTES_API_KEY}"
	 		}
